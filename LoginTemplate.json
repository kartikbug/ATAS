{
    "templates":[
        {
            "template_name":"LoginTemplate",
            "template_actions":[
                {
                    "action_name":"ui_open_browser",
                    "action_config":{
                        "browser_name":"edge"
                    }
                },
                {
                    "action_name":"ui_navigate",
                    "action_config":{
                        "url":"[VM|url]"
                    }
                },
                {
                    "action_name":"ui_input",
                    "action_config":{
                        "object_name":"EMAIL",
                        "clear_input":"yes",
                        "input_value":"[VM|username]"
                    }
                },
                {
                    "action_name":"ui_input",
                    "action_config":{
                        "object_name":"PASSWORD",
                        "clear_input":"yes",
                        "input_value":"[VM|password]"
                    }
                },
                {
                    "action_name":"ui_click",
                    "action_config":{
                        "object_name":"LOGIN_BTN"
                    }
                }
            ]
        }
    ]
}